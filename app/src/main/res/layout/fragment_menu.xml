<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".BarraNav.MenuFragment"
    android:background="@color/white">

<androidx.constraintlayout.widget.ConstraintLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    >
    <TextView
        android:id="@+id/casino_univ"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginStart="20dp"
        android:layout_marginTop="20dp"
        android:fontFamily="@font/bree_serif"
        android:textSize="24sp"
        android:text="@string/casino_universidad"
        android:textAppearance="@style/elige_una_o"
        android:gravity="bottom"
        />
    <View
        android:id="@+id/image_6"
        android:layout_width="104dp"
        android:layout_height="59dp"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginEnd="20dp"
        android:layout_marginTop="20dp"
        android:background="@drawable/logo"
        />
    <TextView
        android:id="@+id/elige_una_o"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/casino_univ"
        android:layout_marginStart="20dp"
        android:layout_marginTop="20dp"
        android:fontFamily="@font/bree_serif"
        android:textSize="24sp"
        android:text="Seleccione una categorÃ­a:"
        android:textAppearance="@style/elige_una_o"
        android:gravity="top"
        />
    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/menubebida"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginTop="20dp"
        android:paddingTop="12dp"
        style="@style/Botones"
        android:drawableTop="@drawable/i_bebidas"
        android:text="@string/bebidas"
        android:textColor="@color/grisoscuro"
        android:textSize="18sp"
        android:fontFamily="@font/bree_serif"
        android:gravity="bottom|center"
        app:layout_constraintEnd_toStartOf="@id/menupostre"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/elige_una_o"
        android:onClick="iraBebidas"
        />
    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/menupostre"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginTop="20dp"
        android:paddingTop="12dp"
        style="@style/Botones"
        android:drawableTop="@drawable/i_postres"
        android:text="@string/postres"
        android:textColor="@color/grisoscuro"
        android:textSize="18sp"
        android:fontFamily="@font/bree_serif"
        android:gravity="bottom|center"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@id/menubebida"
        app:layout_constraintTop_toBottomOf="@id/elige_una_o"
        />
    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/menualmuerzos"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginTop="20dp"
        android:paddingTop="12dp"
        style="@style/Botones"
        android:drawableTop="@drawable/i_almuerzos"
        android:text="@string/almuerzos"
        android:textColor="@color/grisoscuro"
        android:textSize="18sp"
        android:fontFamily="@font/bree_serif"
        android:gravity="bottom|center"
        app:layout_constraintEnd_toStartOf="@id/menufrutas"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/menubebida"
        />
    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/menufrutas"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginTop="20dp"
        android:paddingTop="12dp"
        style="@style/Botones"
        android:drawableTop="@drawable/i_frutas"
        android:text="@string/frutas"
        android:textColor="@color/grisoscuro"
        android:textSize="18sp"
        android:fontFamily="@font/bree_serif"
        android:gravity="bottom|center"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@id/menualmuerzos"
        app:layout_constraintTop_toBottomOf="@id/menupostre"
        />
    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/menucomidarapida"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginTop="20dp"
        android:paddingTop="1dp"
        style="@style/Botones"
        android:drawableTop="@drawable/i_comrapida"
        android:text="@string/comida_nr_pida"
        android:textColor="@color/grisoscuro"
        android:textSize="18sp"
        android:lineSpacingMultiplier="0.6"
        android:fontFamily="@font/bree_serif"
        android:gravity="bottom|center"
        app:layout_constraintEnd_toStartOf="@id/menupostre"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/menualmuerzos"
        />
    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/menugalletas"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginTop="20dp"
        android:paddingTop="12dp"
        style="@style/Botones"
        android:drawableTop="@drawable/i_galletas"
        android:text="@string/galletas"
        android:textColor="@color/grisoscuro"
        android:textSize="18sp"
        android:fontFamily="@font/bree_serif"
        android:gravity="bottom|center"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@id/menucomidarapida"
        app:layout_constraintTop_toBottomOf="@id/menufrutas"
        />
    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/menusandwich"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginTop="20dp"
        android:paddingTop="12dp"
        style="@style/Botones"
        android:drawableTop="@drawable/i_sandwich"
        android:text="@string/sandwich"
        android:textColor="@color/grisoscuro"
        android:textSize="18sp"
        android:fontFamily="@font/bree_serif"
        android:gravity="bottom|center"
        app:layout_constraintEnd_toStartOf="@id/menutecafe"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/menucomidarapida"
        />
    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/menutecafe"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginTop="20dp"
        android:paddingTop="12dp"
        style="@style/Botones"
        android:drawableTop="@drawable/i_tecafe"
        android:text="@string/t_caf"
        android:textColor="@color/grisoscuro"
        android:textSize="18sp"
        android:fontFamily="@font/bree_serif"
        android:gravity="bottom|center"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@id/menusandwich"
        app:layout_constraintTop_toBottomOf="@id/menugalletas"
        />
</androidx.constraintlayout.widget.ConstraintLayout>

</FrameLayout>